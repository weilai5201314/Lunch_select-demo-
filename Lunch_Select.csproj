<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net7.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
      <Reference Include="MySql.Data">
        <HintPath>C:\Program Files (x86)\MySQL\MySQL Connector NET 8.0.32\Assemblies\netstandard2.1\MySql.Data.dll</HintPath>
      </Reference>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="mysql-connector-net" Version="1.0.0" />
      <PackageReference Include="MySql.Data" Version="8.0.33" />
      <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="ConsoleApp1\**" />
      <Compile Remove="ConsoleApp3\**" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Remove="ConsoleApp1\**" />
      <EmbeddedResource Remove="ConsoleApp3\**" />
    </ItemGroup>

    <ItemGroup>
      <Page Remove="ConsoleApp1\**" />
      <Page Remove="ConsoleApp3\**" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="ConsoleApp1\**" />
      <None Remove="ConsoleApp3\**" />
    </ItemGroup>

</Project>
